{
  "manifest_version": 3,
  "name": "Form Scanner",
  "version": "1.0",
  "description": "Scans forms on the page and returns labels, names, placeholders, types and ids of inputs.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "Form Scanner",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}
